<div>
  <div>
    <div>
      <h4>注文履歴一覧</h4>

      <table class="table">
        <tr>
          <th>購入日時</th>
          <th>購入者</th>
          <th>注文個数</th>
          <th>注文ステータス</th>
        </tr>

        <% @orders.each do |order| %>
          <tr>
            <td><%= order.created_at %></td>
            <td><%= order.customer.last_name+order.customer.first_name %></td>
            <td></td>
            <td></td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>
